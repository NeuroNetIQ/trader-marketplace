{
  "name": "@neuronetiq/marketplace-cli",
  "version": "0.2.2",
  "description": "CLI tool for interacting with the NeuroNetIQ ML Marketplace",
  "type": "module",
  "bin": {
    "mp": "./bin/mp.js"
  },
  "files": [
